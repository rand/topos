{
  "name": "tree-sitter-topos",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Topos",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "parser",
    "tree-sitter",
    "topos"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.tps"
  ],
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.25.0"
  },
  "tree-sitter": [
    {
      "scope": "source.topos",
      "file-types": [
        "tps",
        "topos"
      ],
      "injection-regex": "^topos$",
      "highlights": "queries/highlights.scm"
    }
  ]
}
